    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/listOrderWatingConfirm"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </androidx.recyclerview.widget.RecyclerView>


</LinearLayout>